{
    "host": "sample-snmpv3-authNoPriv",
    "name": "",
    "status": "enable",
    "proxy": "",
    "description": "sample\\r\\nsnmpv3-authNoPriv",
    "templates": [
        "ICMP Ping",
        "Generic SNMP"
    ],
    "groups": [
        "Discovered hosts"
    ],
    "interfaces": [
        {
            "type": "snmp",
            "main": "default",
            "useip": "ip",
            "ip": "127.0.0.1",
            "dns": "",
            "port": "161",
            "details": {
                "version": "3",
                "bulk": "on",
                "community": "",
                "contextname": "{$CONTEXTNAME}",
                "securityname": "{$SECURITYNAME}",
                "securitylevel": "authNoPriv",
                "authprotocol": "MD5",
                "authpassphrase": "{$AUTHPASS}",
                "privprotocol": "",
                "privpassphrase": ""
            }
        }
    ],
    "tags": [
        {
            "tag": "Sample",
            "value": "snmpv3"
        }
    ],
    "macros": [
        {
            "macro": "{$CONTEXTNAME}",
            "value": "contextname",
            "type": "text"
        },
        {
            "macro": "{$SECURITYNAME}",
            "value": "securityname",
            "type": "text"
        },
        {
            "macro": "{$AUTHPASS}",
            "value": "authpass",
            "type": "secret"
        }
    ],
    "inventory": {
        "mode": "auto",
        "inventory": []
    },
    "encryption": {
	"tls_connect": "no",
	"tls_accept": "no",
	"tls_issuer": "",
	"tls_subject": "",
	"tls_psk_identity": "",
	"tls_psk": ""
    }
}
